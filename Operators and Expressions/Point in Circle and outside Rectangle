var radius = 3;
var centerX = 1;
var centerY = 1;



function inCircle(pointX, pointY){
	var point = Math.sqrt((pointX-centerX)*(pointX-centerX) + (pointY-centerY)*(pointY-centerY));
	if((point <= radius) && ((pointX < -1 || pointX > 5) || (pointY > 1 || pointY < -1))){
		console.log("true");
	}
	else{
		console.log("false");
	}
}

inCircle(-100, -100);
